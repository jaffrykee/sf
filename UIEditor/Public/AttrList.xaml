<Grid
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:userView="clr-namespace:UIEditor"
	mc:Ignorable="d"
	x:Class="UIEditor.AttrList"
	x:Name="mx_root"
	Loaded="mx_root_Loaded"
	Background="#ff666666">
	<StackPanel Margin="2,2,2,2">
		<Border BorderThickness="1" BorderBrush="#ffffffff" CornerRadius="2" Background="#ff444444">
			<Label Foreground="#ffffffff" Margin="0,0,0,0" Padding="5,1,0,1" Name="mx_title"></Label>
		</Border>
		<StackPanel Name="mx_frame">
		</StackPanel>
	</StackPanel>
</Grid>